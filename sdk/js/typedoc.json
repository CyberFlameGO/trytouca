{
  "name": "Touca SDK for JavaScript",
  "out": "./local/docs",
  "entryPoints": ["packages/node/src"],
  "includes": ["packages/node"],
  "exclude": [
    "**/*.js",
    "**/coverage/**/*",
    "**/dist/**/*",
    "**/node_modules/**/*",
    "**/test/**/*"
  ],
  "disableSources": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "hideGenerator": true,
  "includeVersion": true,
  "readme": "README.md",
  "sort": ["visibility", "static-first", "source-order"],
  "theme": "default"
}
